  "FeaturesTraits": [
    {
      "characterId": "74702535-ba9d-4458-a791-9bb7a3838b0a",
      "background": "I see omens in every event and action.  The gods try to speak to us, we just need to listen.",
      "ideals": "We must help bring about the changes the gods are constantly working in the world.",
      "flaws": "My piety sometimes leads to blindly trust those who profess faith in my god.",
      "bonds": "I owe my life to the priest who took me in when my parents died (Frederick Brightwood)."
    }
  ],
